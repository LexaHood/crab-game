{
  "name": "crab-game",
  "private": true,
  "version": "0.9.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/crab-game.umd.cjs",
  "module": "./dist/crab-game.js",
  "exports": {
    ".": {
      "import": "./dist/crab-game.js",
      "require": "./dist/crab-game.umd.cjs"
    }
  },
  "scripts": {
    "dev": "tsc && vite build",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "todo": "leasot 'src/**' --ignore '**/assets/**'",
    "todo-cli": "leasot -x -r 'markdown' 'src/**' --ignore '**/assets/**' > TODO.md",
    "postinstall": "husky install"
  },
  "release-it": {
    "npm": {
      "publish": false
    }
  },
  "dependencies": {
    "@preact/signals": "^1.1.3",
    "classnames": "^2.3.2",
    "preact": "^10.11.3",
    "preact-custom-element": "^4.2.1",
    "react-use": "^17.4.0",
    "sass": "^1.58.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/node": "^18.11.18",
    "@types/preact-custom-element": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "leasot": "^13.3.0",
    "release-it": "^15.10.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
